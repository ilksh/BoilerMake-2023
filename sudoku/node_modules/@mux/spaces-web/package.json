{
  "name": "@mux/spaces-web",
  "version": "1.1.1",
  "scripts": {
    "prebuild": "genversion --es6 --semi src/version.ts",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs && yarn build:iife && yarn build:types",
    "build:esm": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=esm --outdir=dist --out-extension:.js=.mjs --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:cjs": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=cjs --outdir=dist --out-extension:.js=.cjs --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:iife": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=iife --outfile=dist/mux-spaces.js --global-name=MuxSpaces --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "example": "yarn prebuild && DEV=true NODE_ENV=staging parcel ./example/index.html --port 3001",
    "example-local": "yarn prebuild && DEV=true parcel ./example/index.html --port 3001",
    "example-prod": "yarn prebuild && DEV=true NODE_ENV=production parcel ./example/index.html --port 3001",
    "lint": "eslint src/* && prettier --check src/*",
    "lint-fix": "prettier --write src/* example/*",
    "docs": "typedoc",
    "jest": "jest test/suites/integration test/suites/unit",
    "jest-test-coverage": "jest test/suites/integration test/suites/unit --coverage --collectCoverageFrom=\"src/**/*\"",
    "playwright": "playwright install && playwright test --project=chromium",
    "playwright-staging": "playwright install && NODE_ENV=staging playwright test --project=chromium",
    "playwright-local": "playwright install && NODE_ENV=local playwright test --project=chromium",
    "playwright-prod": "playwright install && NODE_ENV=prod playwright test --project=chromium",
    "dev": "./dev.sh",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@playwright/test": "^1.28.0",
    "@types/events": "^3.0.0",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "axios": "^1.1.3",
    "babel-jest": "^29.3.1",
    "buffer": "^5.5.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.14",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "genversion": "^3.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-websocket-mock": "^2.4.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.1.0",
    "mock-socket": "^9.1.5",
    "parcel": "^2.8.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.21",
    "typedoc-plugin-no-inherit": "^1.4.0",
    "typescript": "^4.9.3",
    "util": "^0.12.5"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/mux-spaces.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "files": [
    "/dist"
  ],
  "targets": {
    "module": {
      "includeNodeModules": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "dependencies": {
    "@testrtc/watchrtc-sdk": "^1.35.2",
    "events": "^3.3.0",
    "uuid": "^9.0.0",
    "webrtc-adapter": "^8.2.0"
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint-fix"
  }
}
